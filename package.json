{
  "name": "jinnytty-gps",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "web/*",
    "cli/*",
    "microservice/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build-p": "turbo run build --filter=./packages/*",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.8",
    "turbo": "^1.9.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.3.1"
}
